  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "_inactive",
        "fontStyle": "italic"
      }
    },

    {
      "scope": ["constant"],
      "settings": {
        "foreground": "_pine"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.charcter.escape"
      ],
      "settings": {
        "foreground": "_rose"
      }
    },

    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "_rose"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "_foam"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {
        "foreground": "_iris",
        "fontStyle": "italic"
      }
    },

    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "_love"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "_subtle"
      }
    },

    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "_pine"
      }
    },

    {
      "scope": ["meta.tag", "meta.brace"],
      "settings": {
        "foreground": "_text"
      }
    },
    {
